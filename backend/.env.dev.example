# MyCoach Backend — Variables d'environnement DEV
# Copier en .env.dev et remplir les valeurs

DATABASE_URL=postgresql+asyncpg://mycoach:mycoach_dev@localhost:5432/mycoach
ENVIRONMENT=development
DEBUG=true

# Générer avec : python -c "from cryptography.fernet import Fernet; print(Fernet.generate_key().decode())"
FIELD_ENCRYPTION_KEY=REMPLACER_PAR_CLE_FERNET_A
TOKEN_ENCRYPTION_KEY=REMPLACER_PAR_CLE_FERNET_B

# Générer avec : python -c "import secrets; print(secrets.token_hex(32))"
API_KEY_SALT=REMPLACER_PAR_SEL_ALEATOIRE
SECRET_KEY=REMPLACER_PAR_SECRET_ALEATOIRE

# Google OAuth2 — https://console.cloud.google.com/
GOOGLE_CLIENT_ID=REMPLACER_PAR_CLIENT_ID_GOOGLE

FRONTEND_URL=http://localhost:3000
