# MyCoach Backend — Variables développement
# cp .env.dev.example .env.dev  (ne pas commiter .env.dev)

DATABASE_URL=postgresql+asyncpg://mycoach:mycoach_dev@postgres:5432/mycoach

SECRET_KEY=dev_secret_key_change_in_production_32chars_min
APP_ENV=development
APP_DEBUG=true
APP_PORT=8000

# Google OAuth (optionnel en dev)
GOOGLE_CLIENT_ID=
GOOGLE_CLIENT_SECRET=

# SMS : "console" en dev (log uniquement, pas de vrai SMS)
SMS_PROVIDER=console

# CORS
CORS_ORIGINS=["http://localhost:8000","http://localhost:3000","http://10.0.2.2:8000"]
